u662f\u4e00\u4e2a\u79ef\u6781\u800c\u5229\u597d\u7684\u4fe1\u53f7\u3002\u800c\u4e2d\u56fd\u4f5c\u4e3a\u5168\u7403\u589e\u957f\u8f83\u5feb\u7684\u94bb\u77f3\u5e02\u573a\uff0c\u5728\u5168\u7403\u91d1\u878d\u5371\u673a\u7684\u80cc\u666f\u4e0b\uff0c2009\u5e74\u5bf9\u94bb\u77f3\u7684\u9700\u6c42\u5e76\u672a\u53d7\u5230\u5f88\u5927\u5f71\u54cd\uff1b\u5728\u672a\u6765\u7ecf\u6d4e\u590d\u82cf\u7684\u9884\u671f\u4e0b\uff0c2010\u5e74\u4e2d\u56fd\u5e02\u573a\u94bb\u77f3\u9500\u552e\u91cf\u5c06\u4f1a\u7ee7\u7eed\u6500\u5347\u3002\u201d\u6b27\u9646\u4e4b\u661f(\u4e0a\u6d77)\u94bb\u77f3\u516c\u53f8\u603b\u7ecf\u7406\u59dc\u6770\u5982\u662f\u8bf4\u3002&nbsp;\n&nbsp;\n\u3000\u3000\u59dc\u6770\u8c08\u9053\uff0c\u4f5c\u4e3a\u8f83\u65e9\u8fdb\u5165\u4e2d\u56fd\u7684\u6210\u54c1\u94bb\u6279\u53d1\u5546\uff0c\u5728\u8fc7\u53bb\u768410\u5e74\u91cc\uff0c\u6b27\u9646\u4e4b\u661f\u59cb\u7ec8\u79ef\u6781\u5730\u4e0e\u5408\u4f5c\u4f19\u4f34\u4e00\u8d77\u643a\u624b\u7ef4\u62a4\u5e02\u573a\u4fe1\u5fc3\uff0c\u5728\u7ecf\u6d4e\u5371\u673a\u5230\u6765\u7684\u65f6\u523b\uff0c\u94bb\u77f3\u884c\u4e1a\u7684\u4efb\u52a1\u662f\u7ef4\u62a4\u94bb\u77f3\u7684\u6d88\u8d39\u4fe1\u5fc3\uff0c\u7a33\u5b9a\u94bb\u77f3\u4ef7\u683c\uff0c\u6269\u5927\u94bb\u77f3\u5e02\u573a\u9700\u6c42\uff0c\u56e0\u4e3a\u94bb\u77f3\u73cd\u8d35\u6c38\u6052\u7684\u5f62\u8c61\u662f\u7ecf\u8fc7\u6570\u4e2a\u4e16\u7eaa\u5efa\u7acb\u8d77\u6765\u7684\uff0c\u4e0d\u80fd\u56e0\u4e3a\u77ed\u671f\u5229\u76ca\u800c\u7834\u574f\u4e86\u6574\u4e2a\u94bb\u77f3\u884c\u4e1a\u7684\u524d\u9014\u3002&nbsp;\n&nbsp;\n\u3000\u3000\u94bb\u77f3\u6295\u8d44\u662f\u540e\u91d1\u878d\u5371\u673a\u65f6\u4ee3\u7684\u7406\u8d22\u65b0\u6e20\u9053&nbsp;\n&nbsp;\n\u3000\u3000\u5e2d\u5377\u5168\u7403\u76842008\u91d1\u878d\u5371\u673a\u867d\u7136\u5df2\u8fc7\u53bb1\u5e74\u591a\u4e86\uff0c\u4f46\u5176\u5bf9\u5168\u7403\u7ecf\u6d4e\u6240\u5e26\u6765\u7684\u8d1f\u9762\u5f71\u54cd\u4ecd\u5728\u7ee7\u7eed\u3002\u8fd1\u671f\uff0c\u968f\u7740\u7f8e\u5143\u6301\u7eed\u8d2c\u503c\u3001\u56fd\u9645\u539f\u6cb9(75.75,-0.26,-0.34%)\u53ca\u9ec4\u91d1(1192.80,4.60,0.39%)\u4ef7\u683c\u4e0d\u65ad\u4e0a\u6da8\uff0c\u5e02\u573a\u4e0a\u5f15\u53d1\u8bb8\u591a\u5173\u4e8e\u901a\u8d27\u81a8\u80c0\u7684\u62c5\u5fc3\u3002\u201c\u4f17\u6240\u5468\u77e5\uff0c\u4e2d\u56fd\u662f\u5b58\u6b3e\u5927\u56fd\uff0c\u9762\u4e34\u901a\u80c0\u5371\u673a\uff0c\u5e7f\u5927\u7684\u6295\u8d44\u8005\u4e5f\u5728\u4e0d\u65ad\u5730\u5bfb\u6c42\u8d44\u4ea7\u4fdd\u503c\u548c\u589e\u503c\u7684\u9014\u5f84\u3002\u201d\u59dc\u6770\u8868\u793a\u3002&nbsp;\n&nbsp;\n\u3000\u3000\u59dc\u6770\u8bf4\uff0c\u6295\u8d44\u623f\u4ea7\u906d\u9047\u653f\u5e9c\u8c03\u63a7\uff0c\u6295\u8d44\u80a1\u5e02\u4e0d\u5e78\u5927\u76d8\u8df3\u6c34\uff0c\u94b1\u5b58\u94f6\u884c\u53c8\u9762\u4e34\u901a\u8d27\u81a8\u80c0\u3002\u4ee5\u5f80\u8f83\u4e3a\u51b7\u95e8\u7684\u94bb\u77f3\u5f02\u519b\u7a81\u8d77\uff0c\u6210\u4e3a\u6295\u8d44\u5e02\u573a\u7684\u65b0\u8d35\u3002\u6709\u6570\u636e\u663e\u793a\uff0c\u4ece\u4eca\u5e74\u5e74\u521d\u8d77\uff0c\u5c31\u6709\u5927\u91cf\u7684\u201c\u70ed\u94b1\u201d\u6084\u6084\u6d8c\u5165\u73e0\u5b9d\u5e02\u573a\uff0c\u94bb\u77f3\u3001\u7fe1\u7fe0\u53ca\u4e00\u4e9b\u5de5\u827a\u54c1\u90fd\u6709\u4e86\u4e0d\u5c0f\u7684\u6da8\u5e45\u3002\u5176\u4e2d\uff0c\u5c24\u4ee5\u514b\u62c9\u94bb\u56e0\u5176\u826f\u597d\u7684\u6d41\u901a\u6027\u3001\u6781\u9ad8\u7684\u5347\u503c\u6f5c\u529b\u53ca\u8f83\u4f4e\u7684\u8d2d\u4e70\u95e8\u69db\u800c\u6210\u4e3a\u6295\u8d44\u8005\u7684\u9996\u9009\u3002 &nbsp;\n&nbsp;\n&nbsp;\n\u94bb\u77f3\u662f\u4e00\u79cd\u9ad8\u5ea6\u6d53\u7f29\u7684\u8d22\u5bcc&nbsp;\n&nbsp;\n\u3000\u3000\u201c\u94bb\u77f3\u662f\u4e00\u79cd\u8d27\u5e01\u5f62\u5f0f\u3002\u201d\u5b89\u7279\u536b\u666e\u94bb\u77f3\u9ad8\u9636\u59d4\u5458\u4f1a\u7684\u9a6c\u514b\u30fb\u51e1\u30fb\u5df4\u54c8\u53f2\u94ce\u8bf4\uff0c\u201c\u5b83\u4eec\u53ef\u7528\u6765\u4e3a\u56fd\u9645\u501f\u8d37\u80cc\u4e66\u3001\u507f\u8fd8\u503a\u52a1\u3001\u652f\u4ed8\u8d3f\u6b3e\u3001\u8d2d\u4e70\u67aa\u68b0\u3002\u5728\u8bb8\u591a\u60c5\u51b5\u4e0b\uff0c\u5b83\u4eec\u6bd4\u94b1\u8fd8\u597d\u7528\u3002\u201d&nbsp;\n&nbsp;\n\u3000\u3000\u59dc\u6770\u8ba4\u4e3a\uff0c\u786e\u5b9e\uff0c\u94bb\u77f3\u662f\u4e00\u79cd\u9ad8\u5ea6\u6d53\u7f29\u7684\u8d22\u5bcc\uff0c\u76f8\u6bd4\u5176\u4ed6\u7684\u8d27\u5e01\u5f62\u5f0f\uff0c\u94bb\u77f3\u4f53\u79ef\u5c0f\uff0c\u91cd\u91cf\u8f7b\uff0c\u65e0\u8bba\u662f\u643a\u5e26\u8fd8\u662f\u4fdd\u5b58\u90fd\u8f83\u4e3a\u5bb9\u6613\u3002\u94bb\u77f3\u662f\u7531\u767e\u5206\u4e4b\u767e\u7eaf\u51c0\u7684\u78b3\u7ec4\u6210\u7684\u81ea\u7136\u5b9d\u77f3\u3002\u5b83\u4eec\u662f\u76ee\u524d\u4eba\u7c7b\u53d1\u73b0\u6700\u575a\u97e7\u7684\u81ea\u7136\u7269\u8d44\uff0c\u540c\u65f6\u4e5f\u662f\u6700\u8d35\u7684\u3002\u5728\u5386\u53f2\u4e0a\uff0c\u8bb8\u591a\u8d35\u65cf\u548c\u6709\u94b1\u4eba\u5728\u7ecf\u5386\u4e86\u4e0d\u65ad\u7684\u81ea\u7136\u53d8\u5316\u548c\u653f\u6cbb\u6d69\u52ab\u540e\uff0c\u4ecd\u7136\u53ef\u4ee5\u62e5\u6709\u8bb8\u591a\u8d22\u5bcc\uff0c\u6700\u4e3b\u8981\u7684\u539f\u56e0\u662f\u56e0\u4e3a\u4ed6\u4eec\u62e5\u6709\u4e86\u6709\u6295\u8d44\u4ef7\u503c\u7684\u94bb\u77f3\u3002\u5373\u4f7f\u5728\u4eca\u5929\uff0c\u5f53\u770b\u8d27\u5546\u5728\u6234\u6bd4\u5c14\u65af\u4e70\u8fdb\u6bdb\u576f\u540e\uff0c\u4e5f\u53ef\u76f4\u63a5\u53bb\u94f6\u884c\u7528\u6bdb\u576f\u8fdb\u884c\u62b5\u62bc\u8d37\u6b3e\u3002&nbsp;\n&nbsp;\n\u3000\u3000\u94bb\u77f3\u6bdb\u576f\u4ef7\u683c\u4fdd\u6301\u7740\u5f3a\u52b2\u7684\u4e0a\u5347\u52bf\u5934&nbsp;\n&nbsp;\n\u3000\u3000\u59dc\u6770\u8c08\u9053\uff0c\u5728\u94bb\u77f3\u884c\u4e1a\u7684\u6e90\u5934\uff0c\u94bb\u77f3\u66f4\u591a\u5730\u88ab\u8d4b\u4e88\u91d1\u878d\u4ea7\u54c1\u7684\u5c5e\u6027\u3002\u4ece1934\u4ee5\u6765\uff0c \u94bb\u77f3\u7684\u4ef7\u683c\u589e\u503c\u5927\u5e45\u8d85\u8fc7\u4e86\u901a\u8d27\u81a8\u80c0\u7387\uff0c\u56e0\u6b64\uff0c\u6295\u8d44\u94bb\u77f3\u53ef\u4ee5\u4fdd\u62a4\u6295\u8d44\u4eba\u7684\u8d44\u672c\u4e0d\u88ab\u901a\u8d27\u81a8\u80c0\u4fb5\u8680\u3002\u94bb\u77f3\u4ef7\u683c\u8fd1\u5e74\u6765\u5728\u4e00\u4e2a\u826f\u6027\u901a\u8d27\u81a8\u80c0\u7684\u671f\u95f4\u6210\u957f\uff0c\u56e0\u6b64\uff0c\u6295\u8d44\u94bb\u77f3\u9664\u4e86\u53ef\u4ee5\u63d0\u4f9b\u8d44\u672c\u7684\u4fdd\u62a4\u5916\uff0c\u94bb\u77f3\u540c\u65f6\u4e5f\u4fdd\u969c\u4e86\u4e2a\u4eba\u8d22\u5bcc\u6295\u8d44\u7684\u79c1\u5bc6\u6027\u3002&nbsp;\n&nbsp;\n\u3000\u3000\u59dc\u6770\u63d0\u8bae\uff0c\u94bb\u77f3\u6709\u522b\u4e8e\u4e00\u822c\u7684\u671f\u8d27\u5546\u54c1\u6295\u8d44\uff0c\u6bcf\u5929\u90fd\u4f1a\u6709\u5267\u70c8\u7684\u4ef7\u683c\u6ce2\u52a8\u3002\u4e5f\u6ca1\u6709\u4efb\u4f55\u7684\u653f\u5e9c\u4f1a\u5806\u79ef\u94bb\u77f3\u5e93\u5b58\uff0c\u56e0\u6b64\uff0c\u5404\u56fd\u653f\u5e9c\u5e76\u4e0d\u4f1a\u6709\u63a7\u5236\u6216\u5f71\u54cd\u94bb\u77f3\u7684\u81ea\u7531\u4ea4\u6613\u5e02\u573a\u3002\u94bb\u77f3\u7684\u4ef7\u683c\uff0c\u4e3b\u8981\u7531\u6234\u6bd4\u5c14\u65af\u6765\u8c03\u63a7\u3002\u65e9\u5e74\u51e0\u4e4e\u5168\u7403\u7684\u94bb\u77f3\u6bdb\u576f\u90fd\u662f\u901a\u8fc7\u6234\u6bd4\u5c14\u65af\u6765\u505a\u4ea4\u6613\u7684\uff0c\u540c\u65f6\u501f\u7531\u4e2d\u592e\u9500\u552e\u7ec4\u7ec7\u7684\u94bb\u77f3\u4f9b\u5e94\u91cf\u6765\u7ef4\u62a4\u5168\u7403\u6027\u4ef7\u683c\u3002\u4e0d\u7ba1\u662f\u5728\u7ecf\u6d4e\u8427\u6761\u6216\u662f\u901a\u8d27\u81a8\u80c0\u65f6\u671f\uff0c\u6234\u6bd4\u5c14\u65af\u90fd\u6210\u529f\u7684\u786e\u5b9a\u4e86\u7a33\u5b9a\u94bb\u77f3\u4ef7\u683c\u7684\u80fd\u529b\u3002\u8fd1\u5e74\u6765\uff0c\u867d\u7136\u6234\u6bd4\u5c14\u65af\u5bf9\u5168\u7403\u7684\u6bdb\u576f\u63a7\u5236\u91cf\u4e0b\u964d\u81f340%\uff0c\u4f46\u5176\u5bf9\u94bb\u77f3\u4ef7\u683c\u7684\u8c03\u63a7\u548c\u5f71\u54cd\u529b\uff0c\u4ecd\u662f\u6574\u4e2a\u884c\u4e1a\u7684\u4f9d\u636e\u548c\u98ce\u5411\u6807\u3002&nbsp;\n&nbsp;\n\u3000\u3000\u8fd1\u671f\uff0c\u9ec4\u91d1\u57282008\u5e741055\u7f8e\u5143\/\u76ce\u53f8\u7684\u57fa\u7840\u4e0a\u7ee7\u7eed\u98d9\u5347\uff0c\u7a81\u7834\u4e861200\u7f8e\u5143\/\u76ce\u53f8\u3002\u540c\u65f6\uff0c\u6234\u6bd4\u5c14\u65af\u7684\u8463\u4e8b\u603b\u7ecf\u7406\u683c\u83b1\u65af\u4e8e2010\u5e744\u670825\u65e5\u5728\u300a\u65f6\u4ee3\u8d22\u7ecf\u300b\u6742\u5fd7\u5ba3\u79f0\uff0c\u94bb\u77f3\u7684\u4ef7\u683c\u5728\u672a\u67655\u5e74\u5c06\u4fdd\u6301\u4e0d\u4f4e\u4e8e\u6bcf\u5e745%\u7684\u589e\u957f\u6bd4\u4f8b\u3002\u4e3a\u4e86\u914d\u5408\u8fd9\u4e00\u8ba1\u5212\uff0c\u81f32011\u5e74\uff0c\u6234\u6bd4\u5c14\u65af\u5c06\u628a\u6bcf\u5e74\u5e02\u573a\u7684\u6bdb\u576f\u4f9b\u5e94\u91cf\u75312008\u5e74\u76844800\u4e07\u514b\u62c9\u7f29\u51cf\u81f34000\u4e07\u514b\u62c9\u3002\u201c\u5728\u5e02\u573a\u4f9b\u5e94\u7d27\u7f3a\u7684\u60c5\u51b5\u4e0b\uff0c\u6bdb\u576f\u548c\u6210\u54c1\u94bb\u7684\u6279\u53d1\u5e02\u573a\u5df2\u7ecf\u611f\u53d7\u5230\u4e86\u4ef7\u683c\u4e0a\u6da8\u7684\u538b\u529b\u3002\u4ece2010\u5e74\u5e74\u521d\uff0c\u94bb\u77f3\u6bdb\u576f\u4ef7\u683c\u5927\u6da820%\uff0c\u5e76\u4ecd\u4fdd\u6301\u7740\u5f3a\u52b2\u7684\u4e0a\u5347\u52bf\u5934\u3002\u201d\u59dc\u6770\u8bf4\u3002 &nbsp;\n&nbsp;\n&nbsp;\n\u3000\u6295\u8d44\u94bb\u77f3\u8981\u6709\u5168\u9762\u4e86\u89e3&nbsp;\n&nbsp;\n\u3000\u3000\u59dc\u6770\u8ba4\u4e3a\uff0c\u4eca\u5929\u8bb8\u591a\u94bb\u77f3\u6295\u8d44\u4eba\u90fd\u5df2\u7ecf\u4ece\u8fc7\u53bb\u7684\u6295\u8d44\u4e2d\u83b7\u5229\u4e86\uff0c\u6700\u4e3b\u8981\u7684\u539f\u56e0\u662f\uff0c\u5728\u5730\u7403\u7684\u81ea\u7136\u7269\u8d44\u8d8a\u6765\u8d8a\u5c11\u7684\u60c5\u51b5\u4e4b\u4e0b\uff0c\u8fd9\u4e9b\u7a00\u6709\u5b9d\u77f3\u7684\u4ef7\u683c\u4e5f\u8d8a\u6765\u8d8a\u9ad8\u6da8\u3002\u4f46\u662f\u6295\u8d44\u4eba\u4e5f\u5fc5\u987b\u6ce8\u610f\u4e00\u4ef6\u4e8b\uff0c\u94bb\u77f3\u5e76\u4e0d\u662f\u4e00\u4e2a\u77ed\u671f\u7684\u6295\u8d44\u5de5\u5177\uff0c\u94bb\u77f3\u7684\u4ef7\u683c\u4e5f\u662f\u4f1a\u6709\u8d77\u4f0f\u7684\u3002\u5728\u8fdb\u5165\u8fd9\u4e2a\u5e02\u573a\u4ee5\u524d\uff0c\u6295\u8d44\u4eba\u5e94\u8be5\u5bf9\u94bb\u77f3\u5e02\u573a\u6709\u4e00\u822c\u7684\u77e5\u8bc6\u53ca\u7406\u89e3\u3002\u4e00\u822c\